{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch connectFour.js",
      "program": "${workspaceFolder}/four-oo/connectFour.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome against localhost",
      "url": "http://localhost:5173",
      "webRoot": "${workspaceFolder}/src"
    }
  ]
}
